			Content="<zml    \t\t\n\t\t\tisPrintEnabled='true'\n\t\t\n\t\t\tbgColor='#F4F4F5'\n\t\t\n\t\t\n\t\t\n>\n\t<layout>\n\t<row>\n\t<column\n   \t\t width='100%'\n   \t>\n\t<form \n\telementName='Form'\n\n\tappLinkName = 'thisapp'\n\tlinkName = 'Daily_Payments1'\n\theightType = 'custom'\n\theightValue = '180'\n\tsearchString = 'Select_Date=${selected_date}'\n\turlType = 'none'\n\tzc_Header = 'false'\n/> \n</column>\n</row><row>\n\t<column\n   \t\t width='100%'\n   \t>\n\t<dsp id='html_snippet' elementName='HTML Snippet'\n bgColor=\"#FFFFFF\" \n>\n\t<![CDATA[htmlpage html_snippet()\ncontent\n&lt;%{\n\ttoday_date = input.selected_date;\n\tif(today_date != null)\n\t{\n\t\tstart_date = today_date + &quot; 00:00:01&quot;;\n\t\tend_date = today_date + &quot; 23:59:59&quot;;\n\t\tdata = List();\n\t\t// Fetch payments for the date range\n\t\tpayment_list = Payment[Added_Time &gt;= start_date &amp;&amp; Added_Time &lt;= end_date];\n\t\t// Map: customer_id -&gt; (Map: item_id -&gt; List of payments)\n\t\ttechnician_map = Map();\n\t\tfor each  payment in payment_list\n\t\t{\n\t\t\titem = payment.Item;\n\t\t\tprofit = payment.Profit;\n\t\t\tif(item != null)\n\t\t\t{\n\t\t\t\ttechnician = item.Technician;\n\t\t\t\tif(technician != null)\n\t\t\t\t{\n\t\t\t\t\tif(!technician_map.containKey(technician))\n\t\t\t\t\t{\n\t\t\t\t\t\ttechnician_map.put(technician,Map());\n\t\t\t\t\t}\n\t\t\t\t\titem_map = technician_map.get(technician);\n\t\t\t\t\tif(!item_map.containKey(item))\n\t\t\t\t\t{\n\t\t\t\t\t\titem_map.put(item,List());\n\t\t\t\t\t}\n\t\t\t\t\tpayment_list_for_item = item_map.get(item);\n\t\t\t\t\tpayment_list_for_item.add(payment);\n\t\t\t\t\titem_map.put(item,payment_list_for_item);\n\t\t\t\t\t// Update the list back in map\n\t\t\t\t\ttechnician_map.put(technician,item_map);\n\t\t\t\t\t// Update the customer map\n\t\t\t\t}\n\t\t\t}\n\t\t}\n\t\t// Build final data structure\n\t\ttechnician_ids = technician_map.keys();\n\t\t// List of keys (customer IDs)\n\t\tfor each  tech_id in technician_ids\n\t\t{\n\t\t\ttech_data = Map();\n\t\t\ttechnician_name = Technician[ID == tech_id].Name;\n\t\t\ttech_data.put(&quot;technician&quot;,technician_name);\n\t\t\titem_list = List();\n\t\t\ttotal_received_amount_for_technician = 0;\n\t\t\t// Initialize the total received amount for the customer\n\t\t\titem_map = technician_map.get(tech_id);\n\t\t\titem_ids = item_map.keys();\n\t\t\t// List of item IDs\n\t\t\tfor each  item_id in item_ids\n\t\t\t{\n\t\t\t\ti = Item[ID == item_id];\n\t\t\t\titem_entry = Map();\n\t\t\t\titem_entry.put(&quot;Item&quot;,i.Job_ID);\n\t\t\t\titem_entry.put(&quot;Model&quot;,i.Model);\n\t\t\t\titem_entry.put(&quot;Repairing Charges&quot;,i.Repairing_Charges);\n\t\t\t\titem_entry.put(&quot;Total Spare Parts Cost&quot;,i.Total_Spare_Parts_Cost);\n\t\t\t\titem_entry.put(&quot;Is Spare Part Cost Accounted&quot;,i.Is_Spare_Part_Cost_Accounted);\n\t\t\t\titem_entry.put(&quot;Balance Amount&quot;,i.Balance_Amount);\n\t\t\t\titem_payment_list = List();\n\t\t\t\tfor each  payment in Payment[ID == item_map.get(item_id)]\n\t\t\t\t{\n\t\t\t\t\ttemp = Map();\n\t\t\t\t\ttemp.put(&quot;Received Amount&quot;,payment.Received_Amount);\n\t\t\t\t\ttemp.put(&quot;Profit&quot;,payment.Profit);\n\t\t\t\t\ttemp.put(&quot;Payment Date&quot;,payment.Added_Time);\n\t\t\t\t\titem_payment_list.add(temp);\n\t\t\t\t\ttotal_received_amount_for_technician = total_received_amount_for_technician + payment.Received_Amount;\n\t\t\t\t\t// Add to the customer total\n\t\t\t\t}\n\t\t\t\titem_entry.put(&quot;Payment Details&quot;,item_payment_list);\n\t\t\t\titem_list.add(item_entry);\n\t\t\t}\n\t\t\ttech_data.put(&quot;items&quot;,item_list);\n\t\t\ttech_data.put(&quot;total_received_amount_for_technician&quot;,total_received_amount_for_technician);\n\t\t\t// Store the total for the customer\n\t\t\tdata.add(tech_data);\n\t\t}\n\t\thtml_table = &quot;&lt;table border=&apos;1&apos; cellpadding=&apos;5&apos; cellspacing=&apos;0&apos;&gt;&quot;;\n\t\thtml_table = html_table + &quot;&lt;tr&gt;&lt;th&gt;Technician&lt;/th&gt;&lt;th&gt;Item&lt;/th&gt;&lt;th&gt;Model&lt;/th&gt;&lt;th&gt;Repairing Charges&lt;/th&gt;&lt;th&gt;Total Spare Parts Cost&lt;/th&gt;&lt;th&gt;Balance Amount&lt;/th&gt;&lt;th&gt;Received Amount&lt;/th&gt;&lt;th&gt;Profit&lt;/th&gt;&lt;th&gt;Payment Date&lt;/th&gt;&lt;/tr&gt;&quot;;\n\t\tfor each  technician_data in data\n\t\t{\n\t\t\ttechnician_name = technician_data.get(&quot;technician&quot;);\n\t\t\titems = technician_data.get(&quot;items&quot;);\n\t\t\ttotal_received_amount_for_technician = technician_data.get(&quot;total_received_amount_for_technician&quot;);\n\t\t\ttech_rowspan = 0;\n\t\t\tfor each  item_data in items\n\t\t\t{\n\t\t\t\ttech_rowspan = tech_rowspan + item_data.get(&quot;Payment Details&quot;).size();\n\t\t\t}\n\t\t\ttech_printed = false;\n\t\t\tfor each  item_data in items\n\t\t\t{\n\t\t\t\titem_name = item_data.get(&quot;Item&quot;);\n\t\t\t\titem_model = item_data.get(&quot;Model&quot;);\n\t\t\t\trepairing_charges = item_data.get(&quot;Repairing Charges&quot;);\n\t\t\t\ttotal_spare_parts_cost = item_data.get(&quot;Total Spare Parts Cost&quot;);\n\t\t\t\tis_spare_part_cost_accounted = item_data.get(&quot;Is Spare Part Cost Accounted&quot;);\n\t\t\t\tbalance_amount = item_data.get(&quot;Balance Amount&quot;);\n\t\t\t\tpayment_details = item_data.get(&quot;Payment Details&quot;);\n\t\t\t\titem_rowspan = payment_details.size();\n\t\t\t\titem_printed = false;\n\t\t\t\tfor each  payment in payment_details\n\t\t\t\t{\n\t\t\t\t\treceived_amount = payment.get(&quot;Received Amount&quot;);\n\t\t\t\t\tprofit = payment.get(&quot;Profit&quot;);\n\t\t\t\t\tpayment_date = payment.get(&quot;Payment Date&quot;);\n\t\t\t\t\thtml_table = html_table + &quot;&lt;tr&gt;&quot;;\n\t\t\t\t\tif(!tech_printed)\n\t\t\t\t\t{\n\t\t\t\t\t\thtml_table = html_table + &quot;&lt;td rowspan=&apos;&quot; + tech_rowspan.toString() + &quot;&apos;&gt;&quot; + technician_name + &quot;&lt;/td&gt;&quot;;\n\t\t\t\t\t\ttech_printed = true;\n\t\t\t\t\t}\n\t\t\t\t\tif(!item_printed)\n\t\t\t\t\t{\n\t\t\t\t\t\thtml_table = html_table + &quot;&lt;td rowspan=&apos;&quot; + item_rowspan.toString() + &quot;&apos;&gt;&quot; + item_name + &quot;&lt;/td&gt;&quot;;\n\t\t\t\t\t\thtml_table = html_table + &quot;&lt;td rowspan=&apos;&quot; + item_rowspan.toString() + &quot;&apos;&gt;&quot; + item_model + &quot;&lt;/td&gt;&quot;;\n\t\t\t\t\t\thtml_table = html_table + &quot;&lt;td rowspan=&apos;&quot; + item_rowspan.toString() + &quot;&apos;&gt;&quot; + repairing_charges.toString() + &quot;&lt;/td&gt;&quot;;\n\t\t\t\t\t\thtml_table = html_table + &quot;&lt;td rowspan=&apos;&quot; + item_rowspan.toString() + &quot;&apos;&gt;&quot; + total_spare_parts_cost.toString() + &quot; &quot; + is_spare_part_cost_accounted.tostring() + &quot;&lt;/td&gt;&quot;;\n\t\t\t\t\t\thtml_table = html_table + &quot;&lt;td rowspan=&apos;&quot; + item_rowspan.toString() + &quot;&apos;&gt;&quot; + balance_amount.toString() + &quot;&lt;/td&gt;&quot;;\n\t\t\t\t\t\titem_printed = true;\n\t\t\t\t\t}\n\t\t\t\t\thtml_table = html_table + &quot;&lt;td&gt;&quot; + received_amount.toString() + &quot;&lt;/td&gt;&quot;;\n\t\t\t\t\thtml_table = html_table + &quot;&lt;td&gt;&quot; + profit.toString() + &quot;&lt;/td&gt;&quot;;\n\t\t\t\t\thtml_table = html_table + &quot;&lt;td&gt;&quot; + payment_date + &quot;&lt;/td&gt;&quot;;\n\t\t\t\t\thtml_table = html_table + &quot;&lt;/tr&gt;&quot;;\n\t\t\t\t}\n\t\t\t}\n\t\t\t// Total received row\n\t\t\thtml_table = html_table + &quot;&lt;tr&gt;&quot;;\n\t\t\thtml_table = html_table + &quot;&lt;td colspan=&apos;6&apos;&gt;&lt;strong&gt;Total Received Amount for &quot; + technician_name + &quot;&lt;/strong&gt;&lt;/td&gt;&quot;;\n\t\t\thtml_table = html_table + &quot;&lt;td colspan=&apos;2&apos;&gt;&lt;strong&gt;&quot; + total_received_amount_for_technician.toString() + &quot;&lt;/strong&gt;&lt;/td&gt;&quot;;\n\t\t\thtml_table = html_table + &quot;&lt;/tr&gt;&quot;;\n\t\t}\n\t\thtml_table = html_table + &quot;&lt;/table&gt;&quot;;\n\t\t// Output the final table\n\t\tinfo html_table;\n\t\t// Or you can assign this HTML to be used elsewhere in your page\n\t}\n\t%&gt;\n&lt;center&gt;&lt;%=html_table%&gt;&lt;/center&gt;\n&lt;%\n\n}%&gt;]]>\n</dsp>\n</column>\n</row>\n</layout>\n</zml>"
		}
