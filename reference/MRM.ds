			validations1 as "validations"
			{
				type =  form
				form = Item

				record event = on add or edit

				on validate
				{
					actions 
					{
						custom deluge script
						(
											total_received_amount = 0;
										for each  payment_entry in input.Payment
										{
											total_received_amount = total_received_amount + payment_entry.Received_Amount;
										}
										if(Payment.Received_Amount <= 0)
										{
											alert "Cannot receive amount 0 or less.";
											cancel submit;
										}
										if(input.Discount == null)
										{
											input.Discount = 0;
										}
										if(input.Repairing_Charges <= 0)
										{
											alert "Repairing Charges should be greater then 0.";
											cancel submit;
										}
										if(input.Discount < 0)
										{
											alert "Discount should be 0 or greater.";
											cancel submit;
										}
										if(input.Discount > input.Repairing_Charges)
										{
											alert "Discount cannot be greater then Repairing Charges.";
											cancel submit;
										}
										if(total_received_amount > input.Repairing_Charges - input.Discount)
										{
											alert "Cannot receive payment more then balance amount";
											cancel submit;
										}
										// if(Blueprint.Current_Stage == "Received")
										// {
										// 	alert "Technician is mandatory";
										// 	cancel submit;
										// }
						)
					}
				}

			}
			make_payment as "make payment"
			{
				type =  form
				form = Payment

				record event = on add or edit

				on success
				{
					actions 
					{
						custom deluge script
						(
											item = Item[ID == input.Item];
										total_received_amount = 0;
										balance_amount = 0;
										for each  payment_entry in item.Payment
										{
											total_received_amount = total_received_amount + payment_entry.Received_Amount;
										}
										item.Total_Received_Amount=total_received_amount;
										balance_amount = item.Repairing_Charges - item.Discount - total_received_amount;
										item.Balance_Amount=balance_amount;
										// Item's Payment Status update
										if(item.Balance_Amount == 0)
										{
											item.Payment_Status="Paid";
										}
										else if(item.Balance_Amount > 0 && item.Balance_Amount < item.Repairing_Charges - item.Discount)
										{
											item.Payment_Status="Partially Paid";
										}
										else if(item.Blueprint.Current_Stage == "Repaired Picked up")
										{
											item.Payment_Status="Unpaid";
										}
						)
					}
					actions 
					{
						custom deluge script
						(
											item = Item[ID == input.Item];
										if(!item.Is_Spare_Part_Cost_Accounted)
										{
											input.Profit = input.Received_Amount - item.Total_Spare_Parts_Cost;
											item.Is_Spare_Part_Cost_Accounted=true;
										}
										else
										{
											input.Profit = input.Received_Amount;
										}
						)
					}
				}

			}
