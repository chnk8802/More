		page RM_Dashboard
		{
			displayname = "RM Dashboard"
			Content="<zml    \t\t\n\t\t\n\t\t\n\t\t\twebDeviceConfig='{\"layout\":{\"displayType\":\"plain\",\"design\":\"fluid\",\"style\":\"padding-right:0px;padding-top:0px;padding-bottom:0px;padding-left:0px;\"}}'\n\t\t\n\t\t\n>\n\t<layout>\n\t<row>\n\t<column\n   \t\t width='100%'\n   \t>\n\t<dsp id='html_snippet2' elementName='HTML Snippet'\n bgColor=\"#FFFFFF\" \n>\n\t<![CDATA[htmlpage html_snippet2()\ncontent\n&lt;%{\n\t// Fetching counts dynamically with Added_User criteria for personalized view\n\treceived_count = Item[Blueprint.Current_Stage == &quot;Received&quot; &amp;&amp; Added_User == zoho.loginuser].count();\n\trepairing_count = Item[Blueprint.Current_Stage == &quot;Repairing&quot; &amp;&amp; Added_User == zoho.loginuser].count();\n\trepaired_count = Item[Blueprint.Current_Stage == &quot;Repaired&quot; &amp;&amp; Added_User == zoho.loginuser].count();\n\tnot_repaired_count = Item[Blueprint.Current_Stage == &quot;Not Repaired&quot; &amp;&amp; Added_User == zoho.loginuser].count();\n\t// Combining specific pick-up stages\n\trepaired_picked_count = Item[Blueprint.Current_Stage == &quot;Repaired Picked up&quot; || Blueprint.Current_Stage == &quot;Repair Picked Unpaid&quot; &amp;&amp; Added_User == zoho.loginuser].count();\n\tnot_repaired_picked_count = Item[Blueprint.Current_Stage == &quot;Not Repaired Picked up&quot; &amp;&amp; Added_User == zoho.loginuser].count();\n\t// Financial counts with Added_User criteria\n\tpaid_count = Item[Payment_Status == &quot;Paid&quot; &amp;&amp; Added_User == zoho.loginuser].count();\n\tunpaid_count = Item[Payment_Status == &quot;Unpaid&quot; &amp;&amp; Added_User == zoho.loginuser].count();\n\tpartially_paid_count = Item[Payment_Status == &quot;Partially Paid&quot; &amp;&amp; Added_User == zoho.loginuser].count();\n\tnone_paid_count = Item[Payment_Status == &quot;Payment None&quot; || Payment_Status == &quot;&quot; &amp;&amp; Added_User == zoho.loginuser].count();\n\t%&gt;\n&lt;div class=&quot;dashboard-shell&quot;&gt;\n    &lt;div class=&quot;header-section&quot;&gt;\n        &lt;div class=&quot;title-group&quot;&gt;\n            &lt;span class=&quot;badge&quot;&gt;User Session: &lt;%=zoho.loginuser%&gt;&lt;/span&gt;\n            &lt;h1&gt;My Service Desk&lt;/h1&gt;\n        &lt;/div&gt;\n    &lt;/div&gt;\n\n    &lt;div class=&quot;grid-container&quot;&gt;\n        &lt;h2 class=&quot;section-title&quot;&gt;Service Operations&lt;/h2&gt;\n\n        &lt;a href=&quot;#Report:Received&quot; class=&quot;modern-card&quot;&gt;\n            &lt;div class=&quot;accent-line indigo&quot;&gt;&lt;/div&gt;\n            &lt;div class=&quot;card-body&quot;&gt;\n                &lt;span class=&quot;status-pill&quot;&gt;Active&lt;/span&gt;\n                &lt;div class=&quot;card-main&quot;&gt;\n                    &lt;span class=&quot;card-count&quot;&gt;&lt;%=received_count%&gt;&lt;/span&gt;\n                    &lt;span class=&quot;card-label&quot;&gt;Received&lt;/span&gt;\n                &lt;/div&gt;\n                &lt;div class=&quot;card-footer&quot;&gt;Browse details \u2192&lt;/div&gt;\n            &lt;/div&gt;\n        &lt;/a&gt;\n\n        &lt;a href=&quot;#Report:Repairing&quot; class=&quot;modern-card&quot;&gt;\n            &lt;div class=&quot;accent-line purple&quot;&gt;&lt;/div&gt;\n            &lt;div class=&quot;card-body&quot;&gt;\n                &lt;span class=&quot;status-pill&quot;&gt;In Progress&lt;/span&gt;\n                &lt;div class=&quot;card-main&quot;&gt;\n                    &lt;span class=&quot;card-count&quot;&gt;&lt;%=repairing_count%&gt;&lt;/span&gt;\n                    &lt;span class=&quot;card-label&quot;&gt;Repairing&lt;/span&gt;\n                &lt;/div&gt;\n                &lt;div class=&quot;card-footer&quot;&gt;Browse details \u2192&lt;/div&gt;\n            &lt;/div&gt;\n        &lt;/a&gt;\n\n        &lt;a href=&quot;#Report:Repaired&quot; class=&quot;modern-card&quot;&gt;\n            &lt;div class=&quot;accent-line green&quot;&gt;&lt;/div&gt;\n            &lt;div class=&quot;card-body&quot;&gt;\n                &lt;span class=&quot;status-pill&quot;&gt;Ready&lt;/span&gt;\n                &lt;div class=&quot;card-main&quot;&gt;\n                    &lt;span class=&quot;card-count&quot;&gt;&lt;%=repaired_count%&gt;&lt;/span&gt;\n                    &lt;span class=&quot;card-label&quot;&gt;Repaired&lt;/span&gt;\n                &lt;/div&gt;\n                &lt;div class=&quot;card-footer&quot;&gt;Browse details \u2192&lt;/div&gt;\n            &lt;/div&gt;\n        &lt;/a&gt;\n\n        &lt;a href=&quot;#Report:Not_Repaired&quot; class=&quot;modern-card&quot;&gt;\n            &lt;div class=&quot;accent-line amber&quot;&gt;&lt;/div&gt;\n            &lt;div class=&quot;card-body&quot;&gt;\n                &lt;span class=&quot;status-pill&quot;&gt;Action Required&lt;/span&gt;\n                &lt;div class=&quot;card-main&quot;&gt;\n                    &lt;span class=&quot;card-count&quot;&gt;&lt;%=not_repaired_count%&gt;&lt;/span&gt;\n                    &lt;span class=&quot;card-label&quot;&gt;Not Repaired&lt;/span&gt;\n                &lt;/div&gt;\n                &lt;div class=&quot;card-footer&quot;&gt;Browse details \u2192&lt;/div&gt;\n            &lt;/div&gt;\n        &lt;/a&gt;\n        \n        &lt;a href=&quot;#Report:Repaired_Picked_up&quot; class=&quot;modern-card&quot;&gt;\n            &lt;div class=&quot;accent-line blue&quot;&gt;&lt;/div&gt;\n            &lt;div class=&quot;card-body&quot;&gt;\n                &lt;span class=&quot;status-pill&quot;&gt;Archived&lt;/span&gt;\n                &lt;div class=&quot;card-main&quot;&gt;\n                    &lt;span class=&quot;card-count&quot;&gt;&lt;%=repaired_picked_count%&gt;&lt;/span&gt;\n                    &lt;span class=&quot;card-label&quot;&gt;Repaired Picked up&lt;/span&gt;\n                &lt;/div&gt;\n                &lt;div class=&quot;card-footer&quot;&gt;Browse details \u2192&lt;/div&gt;\n            &lt;/div&gt;\n        &lt;/a&gt;\n        \n        &lt;a href=&quot;#Report:Not_Repaired_Picked_up&quot; class=&quot;modern-card&quot;&gt;\n            &lt;div class=&quot;accent-line red&quot;&gt;&lt;/div&gt;\n            &lt;div class=&quot;card-body&quot;&gt;\n                &lt;span class=&quot;status-pill&quot;&gt;Closed&lt;/span&gt;\n                &lt;div class=&quot;card-main&quot;&gt;\n                    &lt;span class=&quot;card-count&quot;&gt;&lt;%=not_repaired_picked_count%&gt;&lt;/span&gt;\n                    &lt;span class=&quot;card-label&quot;&gt;Not Repaired Picked up&lt;/span&gt;\n                &lt;/div&gt;\n                &lt;div class=&quot;card-footer&quot;&gt;Browse details \u2192&lt;/div&gt;\n            &lt;/div&gt;\n        &lt;/a&gt;\n\n        &lt;h2 class=&quot;section-title&quot;&gt;Financial Overview&lt;/h2&gt;\n\n        &lt;a href=&quot;#Report:Paid&quot; class=&quot;modern-card&quot;&gt;\n            &lt;div class=&quot;accent-line green&quot;&gt;&lt;/div&gt;\n            &lt;div class=&quot;card-body&quot;&gt;\n                &lt;span class=&quot;status-pill&quot;&gt;Financial&lt;/span&gt;\n                &lt;div class=&quot;card-main&quot;&gt;\n                    &lt;span class=&quot;card-count&quot;&gt;&lt;%=paid_count%&gt;&lt;/span&gt;\n                    &lt;span class=&quot;card-label&quot;&gt;Paid&lt;/span&gt;\n                &lt;/div&gt;\n                &lt;div class=&quot;card-footer&quot;&gt;View transactions \u2192&lt;/div&gt;\n            &lt;/div&gt;\n        &lt;/a&gt;\n\n        &lt;a href=&quot;#Report:Unpaid&quot; class=&quot;modern-card&quot;&gt;\n            &lt;div class=&quot;accent-line red&quot;&gt;&lt;/div&gt;\n            &lt;div class=&quot;card-body&quot;&gt;\n                &lt;span class=&quot;status-pill&quot;&gt;Financial&lt;/span&gt;\n                &lt;div class=&quot;card-main&quot;&gt;\n                    &lt;span class=&quot;card-count&quot;&gt;&lt;%=unpaid_count%&gt;&lt;/span&gt;\n                    &lt;span class=&quot;card-label&quot;&gt;Unpaid&lt;/span&gt;\n                &lt;/div&gt;\n                &lt;div class=&quot;card-footer&quot;&gt;View aging report \u2192&lt;/div&gt;\n            &lt;/div&gt;\n        &lt;/a&gt;\n\n        &lt;a href=&quot;#Report:Partially_Paid&quot; class=&quot;modern-card&quot;&gt;\n            &lt;div class=&quot;accent-line blue&quot;&gt;&lt;/div&gt;\n            &lt;div class=&quot;card-body&quot;&gt;\n                &lt;span class=&quot;status-pill&quot;&gt;Financial&lt;/span&gt;\n                &lt;div class=&quot;card-main&quot;&gt;\n                    &lt;span class=&quot;card-count&quot;&gt;&lt;%=partially_paid_count%&gt;&lt;/span&gt;\n                    &lt;span class=&quot;card-label&quot;&gt;Partially Paid&lt;/span&gt;\n                &lt;/div&gt;\n                &lt;div class=&quot;card-footer&quot;&gt;Check balances \u2192&lt;/div&gt;\n            &lt;/div&gt;\n        &lt;/a&gt;\n\n        &lt;a href=&quot;#Report:Payment_None&quot; class=&quot;modern-card&quot;&gt;\n            &lt;div class=&quot;accent-line slate&quot;&gt;&lt;/div&gt;\n            &lt;div class=&quot;card-body&quot;&gt;\n                &lt;span class=&quot;status-pill&quot;&gt;Financial&lt;/span&gt;\n                &lt;div class=&quot;card-main&quot;&gt;\n                    &lt;span class=&quot;card-count&quot;&gt;&lt;%=none_paid_count%&gt;&lt;/span&gt;\n                    &lt;span class=&quot;card-label&quot;&gt;Payment None&lt;/span&gt;\n                &lt;/div&gt;\n                &lt;div class=&quot;card-footer&quot;&gt;Verify records \u2192&lt;/div&gt;\n            &lt;/div&gt;\n        &lt;/a&gt;\n    &lt;/div&gt;\n&lt;/div&gt;\n\n&lt;style&gt;\n/* Existing CSS remains the same as previous version */\n.dashboard-shell { padding: 30px; background-color: #f8fafc; font-family: &apos;Segoe UI&apos;, Roboto, sans-serif; color: #1e293b; }\n.header-section { margin-bottom: 32px; }\n.badge { background: #e2e8f0; padding: 4px 10px; border-radius: 100px; font-siz